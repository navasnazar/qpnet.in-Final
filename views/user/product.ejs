                      </div>
                      <div class="plus-minus">
                        <i class="sub" data-feather="minus"></i>
                        <input type="number" required name="quantity" id="quantity" value="1" min="1" max="<%= product[0].quantity %>" />
                        <i class="add" data-feather="plus"></i>
                      </div>
                    </div>
                  </div>

