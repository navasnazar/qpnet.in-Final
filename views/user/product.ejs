                    <% if(product[0].originalPrice != product[0].sellingPrice){ %> 
                      <del>₹<%= product[0].originalPrice %></del>
                    <% } %> 
                    
                    <% if(product[0].offer != 0){ %> 
                      <span style="color: red; margin-left: 20px;">Offer Now: <%= product[0].offer %> %</span> 
                    <% } %> 
                    </div>

